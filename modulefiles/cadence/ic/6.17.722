#conflict               

set CADENCE_HOME $env(CADENCE_HOME)

set home_path   "$CADENCE_HOME/ic/IC-6.17.722"

set module_name [module-info name]
set n [string last / $module_name]
if {$n >= 0} {
	set software_name [string range $module_name 0 $n-1]
} else {
	set software_name $module_name
}

set env_name IC_HOME

if {[module-info mode load]} {
	if {[info exists ::env($env_name)]} {
		if {![string equal $home_path $env($env_name)]} {
			module unload $software_name
		}
	}
}

proc ModulesHelp { } {
	global version prefix
	global module_name prefix
	global software_name prefix

	puts stderr "\t$module_name - loads the $software_name software & application environment"
	puts stderr "\tThis module file will add $module_name to the"
	puts stderr "\tlist of directories that the module command will search"
	puts stderr "\tfor modules.  Place your own module files here."
	puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads IC-6.17.722 environment"

set	version		6.17.722

setenv 					IC_HOME  $home_path
setenv          OA_HOME  $home_path/oa_v22.50.092
prepend-path    PATH 		 $home_path/bin
setenv          OA_UNSUPPORTED_PLAT linux_rhel50_gcc48x
